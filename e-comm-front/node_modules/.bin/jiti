#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/jiti/bin/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/jiti/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules:/home/tashisakyi/projet_perso/e-commerce/node_modules:/home/tashisakyi/projet_perso/node_modules:/home/tashisakyi/node_modules:/home/node_modules:/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/jiti/bin/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/jiti/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules:/home/tashisakyi/projet_perso/e-commerce/node_modules:/home/tashisakyi/projet_perso/node_modules:/home/tashisakyi/node_modules:/home/node_modules:/node_modules:/home/tashisakyi/projet_perso/e-commerce/e-comm-front/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../jiti/bin/jiti.js" "$@"
fi
